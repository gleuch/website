.page-header
  %h1= t('.page_title')
  %p= t('.copy')


#projects-list.container-fluid
  - unless @projects_list.blank?
    - @projects_list.each do |yr,list|
      - unless list.blank?
        %h3= yr
        %ul
          - list.each do |k,project|
            %li
              %pre= project.inspect
  - else
    %p.text-center= t('.none')